--// WHITELIST (lagay mo mga allowed usernames)
local whitelist = {
    ["theyluvzmonsy"] = true,
    ["DarkRPUser"] = true,
    ["tanginanyonglahat87"] = true,
    ["kenshin_121123343"] = true,
    ["Defntaudri"] = true,
    ["GodSake01"] = true,
    ["woqq108"] = true,
    ["Audreymay05"] = true,
    ["soyiihhh"] = true,
    ["monsywest11"] = true
}

--// Variables
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local player = Players.LocalPlayer

-- Check kung whitelisted
if not whitelist[player.Name] then
    return -- Hindi siya allowed, hindi gagana
end

-- Panel reference
local panel = player:WaitForChild("PlayerGui"):WaitForChild("ExtraUI"):WaitForChild("AdminPanel")

-- Initial
panel.Transparency = 0.1
panel.Visible = false

-- Toggle with Q
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end

    if input.KeyCode == Enum.KeyCode.Q then
        panel.Visible = not panel.Visible
    end
end)
